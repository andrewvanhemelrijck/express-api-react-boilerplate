    "production": "npm run build && nodemon server/bin/production"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "~1.17.1",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "nodemon": "^1.17.3"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "jest": "^22.4.3",
    "supertest": "^3.0.0"
  }
}
